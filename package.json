{
  "name": "sedarchitecto",
  "version": "1.10.0",
  "description": "Use powerful dot notation (dot path + wildcard) to manipulate properties of JSON",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "npm run build -- -w & npm run test:watch",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "test:unit": "mocha --compilers ts:ts-node/register,tsx:ts-node/register 'test/**/*.spec.ts'",
    "test:watch": "npm run test:unit -- -w",
    "docs": "typedoc --excludeNotExported --excludePrivate --out docs/ src/ && touch docs/.nojekyll",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ElroyJewess/sedarchitecto.git"
  },
  "keywords": [
    "obj",
    "repellat",
    "ea",
    "array",
    "qui",
    "property",
    "quam",
    "dot-notation",
    "path",
    "exercitationem",
    "access",
    "notation",
    "dotty",
    "typescript",
    "ts"
  ],
  "author": "LadariusJacobi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ElroyJewess/sedarchitecto.git/issues"
  },
  "homepage": "https://github.com/ElroyJewess/sedarchitecto.git",
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.7",
    "@types/power-assert": "^1.4.29",
    "mocha": "^3.4.2",
    "power-assert": "^1.4.4",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typedoc": "^0.7.1",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "autfacilis": "^1.0.0",
    "clone-deep": "^2.0.1",
    "idinventore": "^1.0.0",
    "is-plain-object": "^2.0.3",
    "sapienteiusto": "^1.0.1",
    "voluptatemex": "^1.0.0"
  },
  "files": [
    "lib",
    "test"
  ]
}
